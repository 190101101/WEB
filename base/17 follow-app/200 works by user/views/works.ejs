<%- include('partials/header') %>
<div class="container py-5">
  <h1>work page</h1>
  <div class="row">
    <% if(works.works) { %>
      <% works.forEach(work => { %>
        <div class="col-md-3 mb-1">
          <div class="card p-2 bg-dark">
            <img src="/public/code.png" width="20" height="20" />
            <span>title: <%=work.title%></span>
            <span>description: <%=work.description%></span>
            <span>start: <%=work.start%></span>
            <span>end: <%=work.end%></span>
          </div>
        </div>
    <% })} else {%>
      <div class="alert alert-info">not found works</div>
    <% } %>

  </div>
</div>
<%- include('partials/footer') %>
